{
  "private": true,
  "name": "cards-and-bees",
  "version": "1.0.0",
  "description": "A simple Colyseus multiplayer card game with cute bees.",
  "main": "lib/index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:docker": "docker run -p 2567:2567 palodequeso/cards-and-bees",
    "build": "npm run clean && tsc && node node_modules/copyfiles/copyfiles package.json ./lib && node node_modules/copyfiles/copyfiles arena.env ./lib && npm run build:frontend && npm run build:docker",
    "build:frontend": "webpack --config webpack.config.js",
    "watch:frontend": "webpack --config webpack.config.js --watch",
    "build:docker": "docker build -t palodequeso/cards-and-bees .",
    "clean": "node node_modules/rimraf/bin lib",
    "test": "mocha --require ts-node/register test/**_test.ts --exit --timeout 15000"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.4",
    "@colyseus/testing": "^0.14.21",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/mocha": "^8.2.3",
    "copyfiles": "^2.4.1",
    "mocha": "^9.0.2",
    "rimraf": "^2.7.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@colyseus/arena": "^0.14.22",
    "@colyseus/monitor": "^0.14.20",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "colyseus": "^0.14.20",
    "cors": "^2.8.5",
    "css-loader": "^6.7.3",
    "express": "^4.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2"
  }
}
